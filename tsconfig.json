{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "types": ["node", "playwright"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "skipLibCheck": true
  }
}
